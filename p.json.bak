{
    "name": "nexgen-launchpad",
    "displayName": "Launchpad",
    "description": "The Extension Publishing Control Center. Validate, audit, and ship your VS Code extensions with one click.",
    "version": "0.1.3",
    "publisher": "NexGenMeta",
    "repository": {
        "type": "git",
        "url": "https://github.com/AlwayzPoppin/LaunchPad"
    },
    "icon": "media/icon.png",
    "engines": {
        "vscode": "^1.85.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./out/extension.js",
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "launchpad-explorer",
                    "title": "Launchpad",
                    "icon": "media/icon.png"
                }
            ]
        },
        "views": {
            "launchpad-explorer": [
                {
                    "type": "webview",
                    "id": "launchpad.controlPanel",
                    "name": "Control Panel",
                    "icon": "media/icon.png"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@nexgen/shared-core": "*",
        "@types/node": "18.x",
        "@types/vscode": "1.85.0",
        "ts-loader": "^9.5.1",
        "typescript": "^5.3.3",
        "webpack": "^5.89.0",
        "webpack-cli": "^5.1.4"
    }
}